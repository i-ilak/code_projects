Cell["\<\
We are left with task 2.), which is probably the hardest part. We first \
determine the total angle the bread has travelled when its lowest point hits \
the ground.\
\>", "Text",
 CellChangeTimes->{{3.824877263816944*^9, 3.824877280884445*^9}, {
  3.824877314130571*^9, 3.824877314869443*^9}, {3.8248773471894703`*^9, 
  3.824877364060481*^9}, {3.824877572949812*^9, 
  3.824877622524794*^9}},ExpressionUUID->"8cd8b6be-fe56-4c69-99de-\
e9cef4b3380d"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"AngleAtGround", "[", 
    RowBox[{"sol_", ",", "tZero_", ",", "tGround_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"theta", "[", "t_", "]"}], "=", 
       RowBox[{"Flatten", "[", 
        RowBox[{
         RowBox[{"\[Theta]", "[", "t", "]"}], "/.", "sol"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"\[Theta]0", "=", 
       RowBox[{"Abs", "[", 
        RowBox[{
         RowBox[{"theta", "[", "tZero", "]"}], "-", 
         RowBox[{
          RowBox[{
           RowBox[{"theta", "'"}], "[", "tZero", "]"}], " ", "tZero"}], "+", 
         RowBox[{"theta", "[", "tZero", "]"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"angle", "=", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"theta", "'"}], "[", "tZero", "]"}], " ", "tGround"}], "-", 
        
        RowBox[{"theta", "[", "tZero", "]"}], "+", "\[Theta]0"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Return", "[", 
       RowBox[{"Mod", "[", 
        RowBox[{
         RowBox[{"angle", "[", 
          RowBox[{"[", "1", "]"}], "]"}], ",", 
         RowBox[{"2.0", " ", "Pi"}]}], "]"}], "]"}]}]}], "]"}]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.766151703307293*^9, 3.7661517033385344`*^9}, {
  3.824489722836239*^9, 3.824489813350273*^9}, {3.824489904553564*^9, 
  3.824490006541628*^9}, {3.824491737526469*^9, 3.8244917396199713`*^9}, {
  3.824877632688764*^9, 3.824877705373752*^9}, {3.824878264139883*^9, 
  3.82487826987084*^9}},
 CellLabel->"In[86]:=",ExpressionUUID->"73a919a3-b31e-4790-8753-f1b51125fe61"],

Cell["\<\
If the angle is between \[Pi]/2 and 3\[Pi]/2, the bread lands on the butter \
side. Obviously, the bread could spin around more than once, we therefore \
take the \[Theta] mod 2\[Pi]. We now  write a simple function that returns \
either 0 or 1, depending on if the bread lands on the butter side or not.\
\>", "Text",
 CellChangeTimes->{{3.824877728540254*^9, 
  3.8248778485752287`*^9}},ExpressionUUID->"a3f0a0ce-7f7f-4c7e-847c-\
0baee3fc3acb"],

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "Returns", " ", "1.0", " ", "for", " ", "ButterSide", " ", "and", " ", 
    "0.0", " ", "if", " ", "not"}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"BreadOrButterSide", "[", "phi_", "]"}], ":=", 
    "\[IndentingNewLine]", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"Pi", "/", "2.0"}], "\[LessEqual]", "phi"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"phi", "\[LessEqual]", 
         RowBox[{"3.0", " ", 
          RowBox[{"Pi", "/", "2.0"}]}]}], ",", "1.0", ",", "0.0"}], "]"}], 
      ",", "\[IndentingNewLine]", "0.0"}], "]"}]}], ";"}]}]], "Input",
 CellChangeTimes->{{3.766151703307293*^9, 3.7661517033385344`*^9}, {
  3.824489722836239*^9, 3.824489813350273*^9}, {3.824489904553564*^9, 
  3.824490006541628*^9}, {3.824491737526469*^9, 3.8244917396199713`*^9}, {
  3.824877855519067*^9, 3.824877890952052*^9}},
 CellLabel->"In[87]:=",ExpressionUUID->"00e1dcf2-176e-4d10-b123-81918cc74a28"],

Cell["We now put everything together", "Text",
 CellChangeTimes->{{3.824877916397691*^9, 
  3.824877968447537*^9}},ExpressionUUID->"399e3db5-ec4e-4dbf-bc4a-\
bd33b3c69e4b"],

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "StartingValueList", " ", "is", " ", "an", " ", "vector", " ", "of", " ", 
    "the", " ", "form", " ", 
    RowBox[{"{", 
     RowBox[{"speed", ",", " ", "height"}], "}"}]}], "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"ClearAll", "[", "solution", "]"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"goodStartIntoDay", "[", "StartingValuesList_", "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"solution", "=", 
        RowBox[{"PDESolver", "[", 
         RowBox[{"StartingValuesList", "[", 
          RowBox[{"[", "1", "]"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"f", "[", "t_", "]"}], "=", 
        RowBox[{
         RowBox[{"D", "[", 
          RowBox[{
           RowBox[{"\[Theta]", "[", "t", "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"t", ",", "2"}], "}"}]}], "]"}], "/.", "solution"}]}], 
       ";", "\[IndentingNewLine]", 
       RowBox[{"tContactLoss", "=", 
        RowBox[{"t", "/.", 
         RowBox[{"FindRoot", "[", 
          RowBox[{
           RowBox[{"f", "[", "t", "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"t", ",", "0.1"}], "}"}]}], "]"}]}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"tGround", " ", "=", " ", 
        RowBox[{"timeBreadAtGround", "[", 
         RowBox[{
          RowBox[{"StartingValuesList", "[", 
           RowBox[{"[", "2", "]"}], "]"}], ",", "solution", ",", 
          "tContactLoss"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"\[Phi]", "=", 
        RowBox[{"AngleAtGround", "[", 
         RowBox[{"solution", ",", "tContactLoss", ",", "tGround"}], "]"}]}], 
       ";", "\[IndentingNewLine]", 
       RowBox[{"Return", "[", 
        RowBox[{"BreadOrButterSide", "[", "\[Phi]", "]"}], "]"}], ";"}]}], 
     "\[IndentingNewLine]", "]"}]}]}]}]], "Input",
 CellChangeTimes->{{3.8248779766302233`*^9, 3.824878008101035*^9}, {
  3.824878113815598*^9, 3.824878124660124*^9}, {3.824878176213862*^9, 
